    },
    "devDependencies": {
        "@atomico/exports": "latest",
        "@atomico/markdown": "latest",
        "@atomico/scaffold": "^0.2.0",
        "@uppercod/vite-meta-url": "latest",
        "@uppercod/vite-meta-url-load-css": "latest",
        "typescript": "^4.5.5",
        "vite": "latest"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    }
}
