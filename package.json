{
    "name": "formilk",
    "description": "Design system created with Atomico",
    "version": "0.7.0",
    "type": "module",
    "scripts": {
        "start": "vite --force",
        "build": "vite build --outDir docs",
        "exports": "exports  src/components.js src/options.js src/tokens.js \"src/*/*.jsx\" --types --exports --main components",
        "prepublishOnly": "npm run exports"
    },
    "dependencies": {
        "@atomico/hooks": "^3.25.1",
        "@uppercod/parse": "^0.4.0",
        "atomico": "^1.37.1"
    },
    "devDependencies": {
        "@atomico/exports": "latest",
        "@atomico/markdown": "latest",
        "@uppercod/vite-meta-url": "latest",
        "@uppercod/vite-meta-url-load-css": "latest",
        "doc-elements": "^2.0.2",
        "typescript": "^4.2.4",
        "vite": "^2.6.13"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    }
}
